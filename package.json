{
  "name": "RxPort",
  "description": "RxPort is a library for baking web workers with RxJS",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AStaroverov/actorr.git"
  },
  "author": "AStaroverov",
  "main": "./dist/rxport.umd.cjs",
  "module": "./dist/rxport.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rxport.js",
      "require": "./dist/rxport.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.examples.config.js",
    "build": "tsc --noEmit && vite build --config vite.build.config.js",
    "test": "jest --detectOpenHandles"
  },
  "devDependencies": {
    "@swc/core": "^1.3.38",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.3",
    "jest": "^29.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^2.1.0",
    "web-locks": "^0.0.8"
  },
  "peerDependencies": {
    "rxjs": "^7.8.1"
  }
}
